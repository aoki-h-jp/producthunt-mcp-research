# Product Hunt API Configuration
# Product Hunt API token (required)
# Get your token from: https://api.producthunt.com/v2/oauth/applications
PH_API_TOKEN=your_developer_token_here

# Logging Configuration
LOG_LEVEL=info

# Rate Limiting Configuration
REQUESTS_PER_SECOND=0.056
BURST_LIMIT=3

# Retry Configuration
MAX_RETRIES=5
REQUEST_DELAY=5.0
MAX_DELAY=60000

# Optional Configuration
TIMEOUT=30000
BACKOFF_MULTIPLIER=2.0
